{% extends 'base.html.twig' %}
{% block title %} Photo Galery for series {{object.getDetelName()}} {% endblock %}
{% block body %}
{{ include('@navbars/collectornavbar.twig') }}
<div class="container text-center">
    <h2>Baners for {{object.getDetelName()}}</h2>
    {% for baner in baners %}
    <div>
        <img src="{{baner}}" width="800" height="200">
    </div>
    {% endfor %}
</div>
{% endblock %}