{% extends 'base.html.twig' %}
{% block title %} Photo Galery for series {{ObjectData.getDetelName()}} {% endblock %}
{% block body %}
{{ include('@navbars/collectornavbar.twig') }}
<div class="container text-center">
<h2>Top Stars</h2>
    <div class="container text-center">
        <ul>
    <table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        {% for el in object %}
        <tr>
            <td><a href="{{path('StarInSeriesMovieList',{star:el.dirname,collector:Collector,seriesid:ObjectData.id})}}">{{el.dirname}}</a></td>
        </tr>
        {% endfor %}
    </tbody>
    </table>
        <ul>
    </div>
    {{ knp_pagination_render(object) }}
</div>
{% endblock %}