{% extends 'base.html.twig' %}
{% block title %} {{object.getName()}} Photos {% endblock %}
{% block body %}
{{ include('@navbars/collectornavbar.twig') }}
<div class="container text-center">
    <h2>
        Photo Galery for star 
        <a href={{path('StarDetail',{id:object.id,collector:Collector})}}>{{object.getName()}}</a>
    </h2>
    {% for photo in photos %}
        <img src="{{photo}}" width="250" height="250">
    {% endfor %}
</div>
{% endblock %}